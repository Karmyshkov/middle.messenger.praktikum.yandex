    "types": "./src/types",
    "data": "./src/data",
    "vendor": "./src/vendor",
    "variables": "./src/variables",
    "utils": "./src/utils",
    "styles": "./src/styles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karmyshkov/middle.messenger.praktikum.yandex.git"
