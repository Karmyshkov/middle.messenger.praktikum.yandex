  "bugs": {
    "url": "https://github.com/Karmyshkov/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/Karmyshkov/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "8.18.0",
    "handlebars": "4.7.7",
    "parcel": "2.5.0",
    "parcel-plugin-handlebars-precompile": "1.0.2",
    "postcss-nested": "5.0.6",
    "stylelint": "14.9.1",
    "stylelint-config-standard": "26.0.0",
